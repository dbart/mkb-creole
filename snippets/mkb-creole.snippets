# Some useful Unicode entities
snippet nbsp
	<<entity>>nbsp<</entity>>
snippet mdash
	<<entity>>mdash<</entity>>
snippet ndash
	<<entity>>ndash<</entity>>
snippet amp
	<<entity>>amp<</entity>>
snippet ldquo
	<<entity>>ldquo<</entity>>
snippet rdquo
	<<entity>>rdquo<</entity>>
snippet lsquo
	<<entity>>lsquo<</entity>>
snippet rsquo
	<<entity>>rsquo<</entity>>
snippet reg
	<<entity>>reg<</entity>>
snippet copy
	<<entity>>copy<</entity>
snippet trade
	<<entity>>trade<</entity>>
snippet para
	<<entity>>para<</entity>>
snippet bull
	<<entity>>bull<</entity>>
snippet middot
	<<entity>>middot<</entity>>
snippet sect
	<<entity>>sect<</entity>>

snippet include
	<<include slug="${1:slugname}">>
snippet <<toc
	<<toc>>
snippet user
	<<user username=${1:username}>>
snippet sql
	<<sql inline=False>>
	${1}
	<</sql>>
snippet fixed
	<<fixed>>${1}<</fixed>>
snippet code inline false
	<<code lang=${1:mysql} inline=false>>
	${2}
	<</code>>${3}
snippet code inline true
	<<code lang=${1:mysql} inline=true>>${2}<</code>> ${3}
snippet table Table Style: default (darkheader-nospace-borders)
	<<style class="darkheader-nospace-borders">>
	|= ${1:Header}
	|  ${2:Data}
	<</style>>
	${3}
snippet table Table Style: none
	|= ${1:Header}
	|  ${2:Data}
	${3}
