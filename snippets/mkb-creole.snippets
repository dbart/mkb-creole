snippet style style="color:gray;font-size:0.8em;font-style:italic;"
	<<style style="${1:color:gray;font-size:0.8em;font-style:italic;}">>${2}<</style>>${3}
snippet style style="font-size:0.8em;"
	<<style style="${1:font-size:0.8em;}">>${2}<</style>>${3}

snippet box blue box
	<<style class="bluebox">>
	${1}
	<</style>>

snippet box red box
	<<style class="redbox">>
	${1}
	<</style>>

snippet box green box
	<<style class="greenbox">>
	${1}
	<</style>>

snippet box yellow box
	<<style class="yellowbox">>
	${1}
	<</style>>

snippet box gray box
	<<style class="graybox">>
	${1}
	<</style>>

snippet link
	[[${1:Link}|${2:Link name}]] ${3}
snippet [[
	[[${1:Link}|${2:Link name}]] ${3}
snippet //
	//${1}// ${2}
snippet **
	**${1}** ${2}
snippet --
	<<strike>>${1}<</strike>> ${2}
snippet ^^
	^^${1}^^ ${2}
snippet ,,
	,,${1},, ${2}
snippet ##
	##${1}## ${2}
snippet __
	__${1}__ ${2}
snippet {{{
	{{{${1}}}}${2}

